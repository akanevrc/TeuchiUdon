@export
message = "Hello!";

_start = () -> {
    let hello = "Hello, world!"      in UnityEngine.Debug.Log(hello);
    let hello = "Hello, TeuchiUdon!" in UnityEngine.Debug.Log(hello);
    let hello = "ok";
    UnityEngine.Debug.Log(hello);
    let f1 = x: string -> x;
    let f2 = () -> UnityEngine.Debug.Log(f1("function"));
    f2();
    let f3 = () -> UnityEngine.Debug.Log(message);
    let f4 = f3;
    f4();
    let num = 100      in UnityEngine.Debug.Log(num);
    let num = 100.123  in UnityEngine.Debug.Log(num);
    let ch  = 'a'      in UnityEngine.Debug.Log(ch);
